cd zsh
cd .config/zsh
command -v neofetch
cd ..
sudo rm -rf ~/.config/zsh
stow zsh -t ~
command -v neofetch 2>&1
vim .config/zsh/ZZ-startup.zsh
cp .config/zsh/ZZ-startup.zsh source/repos/dotfiles/zsh/.config/zsh/ZZ-startup.zsh
cd source/repos/dotfiles/zsh/.config/zsh
vim ZZ-startup.zsh
cd ../../..
git add --all
git commit -m "fixes startup file for zsh"
git push
sudo reboot now
sudo apt update
sudo apt install i3
xrandr | grep -w 'connected'
xrandr --output DP-0 --mode 1080x1920+1080+0
xrandr --output DP-0 --rotate right
xrandr --output DP-2 --rotate left
xrandr --output DP-4 --primary
xrandr --output DP-0 --left-of DP-4
xrandr --output DP-2 --right-of DP-4
sudo apt install feh
xprop
sudo cp /etc/i3status.conf ./
sudo chown $USER:$USER ./i3status.conf
vim i3status.conf
xrandr --output DP-0 --rotate right --left-of DP-4
cd source/repos/dotfiles
mkdir .config
mkdir i3
cd i3
vim config
cp ~/.config/i3/i3status.conf ./
vim 01-keybindings-i3.conf
vim 02-modes-i3.conf
vim 04-workspaces-i3.conf
vim 05-settings-i3.conf
cd ~
cd .config
mv i3 i3backup
stow i3 -t ~
cd ~/.config
la i3
cat .config/i3/config
vim .config/i3/config
# Use Mouse+$mod to drag floating windows to their wanted position\
floating_modifier $mod
la
cat 05-settings-i3.conf
cat 00-startup-i3.conf
vim 03-bar-i3.conf
cd .config/i3
xrandr
xrandr --output DP-2 --rotate left --right-of DP-4
vim 00-startup-i3.conf
cd ~/source/repos/dotfiles
cat i3/.config/i3/config
cat i3/.config/i3/03-bar-i3.conf
vim ~/.config/i3/03-bar-i3.conf
exit
